<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="38dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="count"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spProb" />

    <EditText
        android:id="@+id/edit_y"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="6dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="Y"
        android:inputType="number"
        android:textAlignment="center"
        android:textStyle="italic"
        app:layout_constraintBaseline_toBaselineOf="@+id/edit_x4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView11" />

    <EditText
        android:id="@+id/edit_x3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="X3"
        android:inputType="number"
        android:textAlignment="center"
        android:textStyle="italic"
        app:layout_constraintEnd_toStartOf="@+id/textView8"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/edit_x1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="7dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="X1"
        android:inputType="number"
        android:textAlignment="center"
        android:textStyle="italic"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/edit_x4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="27dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="X4"
        android:inputType="number"
        android:textAlignment="center"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/textView11"
        app:layout_constraintStart_toEndOf="@+id/textView8" />

    <EditText
        android:id="@+id/edit_x2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="X2"
        android:inputType="number"
        android:textAlignment="center"
        android:textStyle="italic"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Spinner
        android:id="@+id/spIter"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:entries="@array/iterations"
        android:spinnerMode="dropdown"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Spinner
        android:id="@+id/spProb"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="42dp"
        android:entries="@array/probability"
        app:layout_constraintEnd_toEndOf="@+id/text_d"
        app:layout_constraintStart_toEndOf="@+id/text_b"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="6dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="A*"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/edit_x1"
        app:layout_constraintEnd_toStartOf="@+id/edit_x1"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="3dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="A ="
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/text_a"
        app:layout_constraintEnd_toStartOf="@+id/text_a" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:layout_marginTop="33dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="Result: "
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:layout_marginTop="87dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="Enter constants: "
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_a"
        android:layout_width="35dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="12dp"
        android:fontFamily="sans-serif-smallcaps"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="11dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="+ D*"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/edit_x3"
        app:layout_constraintEnd_toStartOf="@+id/edit_x4"
        app:layout_constraintStart_toEndOf="@+id/edit_x3" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="D ="
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/text_c"
        app:layout_constraintTop_toTopOf="@+id/text_c" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="+ B*"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/edit_x2"
        app:layout_constraintEnd_toStartOf="@+id/edit_x2"
        app:layout_constraintStart_toEndOf="@+id/edit_x1" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="B ="
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/text_b"
        app:layout_constraintStart_toEndOf="@+id/text_a" />

    <TextView
        android:id="@+id/text_c"
        android:layout_width="35dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="93dp"
        android:layout_marginEnd="93dp"
        android:fontFamily="sans-serif-smallcaps"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView15"
        app:layout_constraintEnd_toStartOf="@+id/edit_y"
        app:layout_constraintStart_toStartOf="@+id/text_b" />

    <TextView
        android:id="@+id/text_b"
        android:layout_width="35dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="29dp"
        android:fontFamily="sans-serif-smallcaps"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/textView15"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="@+id/text_a" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="2dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="+ C*"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/edit_x3"
        app:layout_constraintEnd_toStartOf="@+id/edit_x3"
        app:layout_constraintStart_toEndOf="@+id/edit_x2" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="162dp"
        android:layout_marginEnd="162dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="C ="
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/edit_y"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/text_b" />

    <TextView
        android:id="@+id/text_d"
        android:layout_width="35dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="42dp"
        android:fontFamily="sans-serif-smallcaps"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="="
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/edit_x4"
        app:layout_constraintEnd_toStartOf="@+id/edit_y"
        app:layout_constraintStart_toEndOf="@+id/edit_x4" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="14dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="GENETIC ALGORITHM"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="Iterations: "
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="@+id/edit_x1"
        app:layout_constraintStart_toEndOf="@+id/edit_x1"
        app:layout_constraintTop_toBottomOf="@+id/edit_x1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="Probability: "
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@+id/spProb"
        app:layout_constraintTop_toBottomOf="@+id/edit_x3" />

</androidx.constraintlayout.widget.ConstraintLayout>